doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    meta(name='description', content='')
    meta(name='author', content='')
    title SB Admin - Bootstrap Admin Template
    // Bootstrap Core CSS
    link(href='../css/bootstrap.min.css', rel='stylesheet')
    // Custom CSS
    link(href='../css/sb-admin.css', rel='stylesheet')
    // Morris Charts CSS
    link(href='../css/plugins/morris.css', rel='stylesheet')
    // Custom Fonts
    link(href='../font-awesome/css/font-awesome.min.css', rel='stylesheet', type='text/css')
    // HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries
    // WARNING: Respond.js doesn't work if you view the page via file://
    //if lt IE 9
      script(src='https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js')
      script(src='https://oss.maxcdn.com/libs/respond.js/1.4.2/respond.min.js')
  body
    #wrapper
      // Navigation
      nav.navbar.navbar-inverse.navbar-fixed-top(role='navigation')
        // Brand and toggle get grouped for better mobile display
        .navbar-header
          button.navbar-toggle(type='button', data-toggle='collapse', data-target='.navbar-ex1-collapse')
            span.sr-only Toggle navigation
            span.icon-bar
            span.icon-bar
            span.icon-bar
          a.navbar-brand(href='index.html') SB Admin
        // Top Menu Items
        ul.nav.navbar-right.top-nav
          li.dropdown
            a.dropdown-toggle(href='#', data-toggle='dropdown')
              i.fa.fa-user
              |  Nama Institusi 
              b.caret
            ul.dropdown-menu
              li
                a(href='/dashboard')
                  i.fa.fa-fw.fa-user
                  |  Profile
              li.divider
              li
                a(href='/logout')
                  i.fa.fa-fw.fa-power-off
                  |  Log Out
        // Sidebar Menu Items - These collapse to the responsive navigation menu on small screens
        .collapse.navbar-collapse.navbar-ex1-collapse
          ul.nav.navbar-nav.side-nav
            li.active
              a(href='index.html')
                i.fa.fa-fw.fa-dashboard
                |  Dashboard
            li
              a(href='javascript:;', data-toggle='collapse', data-target='#demo')
                i.fa.fa-fw.fa-users
                |  Manage Patients
            li
              a(href='blank-page.html')
                i.fa.fa-fw.fa-sign-out
                |  Log Out
      // /.navbar-collapse
      
      #page-wrapper
        .container-fluid
          // Page Heading
          .row
            .col-lg-12
              h1.page-header
                | Edit Record 
                small Patiens Record Management
              ol.breadcrumb
                li.active
                  i.fa.fa-dashboard
                  |  Dashboard
              form(role='form')
                .form-group
                  label PID / Patien Name
                  select(name='pid').form-control
                    option(value='1') 1
                    option(value='2') 2
                .form-group
                  label IID / Hospital Name
                  select(name='iid').form-control
                    option(value='1') 1
                    option(value='2') 2
                .form-group
                  label Weigth (Kg)
                  input.form-control(type='number' name='Weigth' placeholder="Patient's weigth")
                .form-group
                  label Heigth (cm)
                  input.form-control(type='number' name='Heigth' placeholder="Patient's heigth")
                .form-group
                  label Blood Pressure (BP)
                  input.form-control(type='number' name='blood_pressure' placeholder="Patient's bloode pressure")
                .form-group
                  label Colestrol
                  input.form-control( type='number' name='colestrol' placeholder="Patient's colestrol")
                .form-group
                  label Glucose Level
                  input.form-control( type='number' name='glucose_level' placeholder="Patient's glucose level")
                .form-group
                  label Uric acid
                  input.form-control( type='number' name='uric_acid' placeholder="Patient's uric acid")
                .form-group
                  label Diagnose
                  input.form-control(type='text' name='diagnose' placeholder="Patient's diagnose")
                .form-group
                  label Additional info
                  textarea.form-control(name='additional_info' rows='3')
                button.btn.btn-primary(type='submit') Submit Button
      
      // jQuery
      script(src='../js/jquery.js')
      // Bootstrap Core JavaScript
      script(src='../js/bootstrap.min.js')
      // Morris Charts JavaScript
      script(src='../js/plugins/morris/raphael.min.js')
      script(src='../js/plugins/morris/morris.min.js')
      script(src='../js/plugins/morris/morris-data.js')
